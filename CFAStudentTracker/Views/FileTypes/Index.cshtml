@model IEnumerable<CFAStudentTracker.Models.FileType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeWeight)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeWeight)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FileTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FileTypeID })
        </td>
    </tr>
}

</table>
